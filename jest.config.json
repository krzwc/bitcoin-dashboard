{
  "roots": [
    "<rootDir>/src"
  ],
  "transform": {
    "^.+\\.jsx?$": "babel-jest",
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "setupFiles": ["<rootDir>/setupTests.js"],
  "moduleNameMapper": {
    "^.+\\.(css|less|scss)$": "babel-jest",
    "^.+\\.(jpg|png|gif|svg)$": "identity-obj-proxy"
  }
}